<?xml version="1.0" encoding="utf-8"?>
<nlg:NLGSpec xmlns="http://simplenlg.googlecode.com/svn/trunk/res/xml"
	xmlns:nlg="http://simplenlg.googlecode.com/svn/trunk/res/xml"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://simplenlg.googlecode.com/svn/trunk/res/xml ">

	<Recording name="cch examples">

		<Record name="doc example 1">
			<Document>
				<child xsi:type="SPhraseSpec">
					<subj xsi:type="NPPhraseSpec">
						<headword cat="NOUN">
							<base>transfusion of whole blood</base>
						</headword>
					</subj>
					<vp xsi:type="VPPhraseSpec" PASSIVE="true" TENSE="PRESENT">
						<headword cat="VERB">
							<base>indicate</base>
						</headword>
					</vp>
				</child>
			</Document>
		</Record>

		<Record name="doc example 2">
			<Document>
				<child xsi:type="SPhraseSpec">
					<preMod xsi:type="PPPhraseSpec">
						<headword cat="PREPOSITION">
							<base>as a result of</base>
						</headword>
						<compl xsi:type="NPPhraseSpec">
							<headword cat="NOUN">
								<base>procedure</base>
							</headword>
							<spec xsi:type="WordElement" cat="DETERMINER">
								<base>the</base>
							</spec>
						</compl>
					</preMod>
					<subj xsi:type="NPPhraseSpec">
						<headword cat="NOUN">
							<base>patient</base>
						</headword>
						<spec xsi:type="WordElement" cat="DETERMINER">
							<base>the</base>
						</spec>
					</subj>
					<vp xsi:type="CoordinatedPhraseElement" conj="and">
						<coord xsi:type="VPPhraseSpec" TENSE="PAST">
							<headword cat="VERB">
								<base>have</base>
							</headword>
							<compl xsi:type="NPPhraseSpec">
								<headword cat="NOUN">
									<base>adverse contrast media reaction</base>
								</headword>
								<spec xsi:type="WordElement" cat="DETERMINER">
									<base>a</base>
								</spec>
							</compl>
						</coord>
						<coord xsi:type="VPPhraseSpec" TENSE="PAST">
							<headword cat="VERB">
								<base>have</base>
							</headword>
							<compl xsi:type="NPPhraseSpec">
								<headword cat="NOUN">
									<base>decreased platelet count</base>
								</headword>
								<spec xsi:type="WordElement" cat="DETERMINER">
									<base>a</base>
								</spec>
							</compl>
						</coord>
						<coord xsi:type="VPPhraseSpec" TENSE="PAST">
							<headword cat="VERB">
								<base>go</base>
							</headword>
							<postMod xsi:type="PPPhraseSpec">
								<headword cat="PREPOSITION">
									<base>into</base>
								</headword>
								<compl xsi:type="NPPhraseSpec">
									<headword cat="NOUN">
										<base>cardiogenic shock</base>
									</headword>
								</compl>
							</postMod>
						</coord>
					</vp>
				</child>
			</Document>
		</Record>

		<Record name="doc example 3">
			<Document>
				<child xsi:type="DocumentElement" cat="LIST_ITEM"
					xsi:schemaLocation="http://code.google.com/p/simplenlg/schemas/version1 ">
					<child xsi:type="DocumentElement" cat="SENTENCE"
						xsi:schemaLocation="http://code.google.com/p/simplenlg/schemas/version1">
						<child xsi:type="NPPhraseSpec">
							<headword cat="NOUN">
								<base>normal coronary arteries</base>
							</headword>
						</child>
					</child>
				</child>
				<child xsi:type="DocumentElement" cat="LIST_ITEM"
					xsi:schemaLocation="http://code.google.com/p/simplenlg/schemas/version1">
					<child xsi:type="DocumentElement" cat="SENTENCE"
						xsi:schemaLocation="http://code.google.com/p/simplenlg/schemas/version1">
						<child xsi:type="NPPhraseSpec">
							<headword cat="NOUN">
								<base>normal left heart hemodynamics</base>
							</headword>
						</child>
					</child>
				</child>
				<child xsi:type="DocumentElement" cat="LIST_ITEM"
					xsi:schemaLocation="http://code.google.com/p/simplenlg/schemas/version1">
					<child xsi:type="DocumentElement" cat="SENTENCE"
						xsi:schemaLocation="http://code.google.com/p/simplenlg/schemas/version1">
						<child xsi:type="NPPhraseSpec">
							<headword cat="NOUN">
								<base>normal right heart hemodynamics</base>
							</headword>
						</child>
					</child>
				</child>
			</Document>
		</Record>

		<Record name="doc example 4">
			<Document>
				<child xsi:type="SPhraseSpec">
					<subj xsi:type="NPPhraseSpec">
						<headword cat="ADVERB">
							<base>there</base>
						</headword>
					</subj>
					<vp xsi:type="VPPhraseSpec">
						<headword cat="VERB">
							<base>be</base>
						</headword>
						<compl xsi:type="NPPhraseSpec">
							<preMod xsi:type="CoordinatedPhraseElement" conj=",">
								<coord xsi:type="AdjPhraseSpec">
									<headword cat="ADJECTIVE">
										<base>eccentric</base>
									</headword>
								</coord>
								<coord xsi:type="AdjPhraseSpec">
									<headword cat="ADJECTIVE">
										<base>tubular</base>
									</headword>
								</coord>
							</preMod>
							<headword cat="NOUN">
								<base>restenosis</base>
							</headword>
							<postMod xsi:type="StringElement">
								<val>(18 mm x 1 mm)</val>
							</postMod>
							<spec xsi:type="WordElement" cat="DETERMINER">
								<base>a</base>
							</spec>
						</compl>
						<postMod xsi:type="PPPhraseSpec">
							<preMod xsi:type="VPPhraseSpec" FORM="GERUND">
								<headword cat="VERB">
									<base>extend</base>
								</headword>
							</preMod>
							<headword cat="PREPOSITION">
								<base>from</base>
							</headword>
							<compl xsi:type="NPPhraseSpec">
								<preMod xsi:type="AdjPhraseSpec">
									<headword cat="ADJECTIVE">
										<base>proximal</base>
									</headword>
								</preMod>
								<spec xsi:type="WordElement" cat="DETERMINER">
									<base>the</base>
								</spec>
							</compl>
							<postMod xsi:type="PPPhraseSpec">
								<headword cat="PREPOSITION">
									<base>to</base>
								</headword>
								<compl xsi:type="NPPhraseSpec">
									<preMod xsi:type="AdjPhraseSpec">
										<headword cat="ADJECTIVE">
											<base>mid</base>
										</headword>
									</preMod>
									<headword cat="NOUN">
										<base>right coronary artery</base>
									</headword>
									<spec xsi:type="WordElement" cat="DETERMINER">
										<base>the</base>
									</spec>
								</compl>
							</postMod>
						</postMod>
						<postMod xsi:type="PPPhraseSpec">
							<headword cat="PREPOSITION">
								<base>with</base>
							</headword>
							<compl xsi:type="NPPhraseSpec">
								<headword cat="NOUN">
									<base>TIMI 1 flow</base>
								</headword>
							</compl>
						</postMod>
					</vp>
				</child>
			</Document>
		</Record>

	</Recording>
</nlg:NLGSpec>
