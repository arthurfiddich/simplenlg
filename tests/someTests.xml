		<Record name="punctuation problem 1">
			<Document cat="PARAGRAPH">
				<child xsi:type="SPhraseSpec">
					<subj xsi:type="NPPhraseSpec">
						<head cat="ADVERB" base="there" />
					</subj>
					<vp xsi:type="VPPhraseSpec">
						<head cat="VERB" base="be" />
						<compl xsi:type="NPPhraseSpec">
							<!--							<preMod xsi:type="StringElement">-->
							<!--								<val>an</val>-->
							<!--							</preMod>-->
							<spec xsi:type="WordElement" cat="DETERMINER" base="an" />
							<preMod xsi:type="StringElement">
								<val>80 %</val>
							</preMod>
							<head cat="NOUN" base="in-stent restenosis" />
						</compl>
						<postMod xsi:type="PPPhraseSpec">
							<head cat="PREPOSITION" base="in" />
							<compl xsi:type="NPPhraseSpec">
								<preMod xsi:type="AdjPhraseSpec">
									<head cat="ADJECTIVE" base="proximal" />
								</preMod>
								<head cat="NOUN" base="right coronary artery" />
								<spec xsi:type="WordElement" cat="DETERMINER" base="the" />
							</compl>
						</postMod>
					</vp>
				</child>
			</Document>

		</Record>
		<Record name="punctuation problem 2">
			<Document cat="PARAGRAPH">
				<child xsi:type="SPhraseSpec">
