		<Record name="Lexical and spelling variants">
			<Document cat="PARAGRAPH">
				<child xsi:type="SPhraseSpec">
					<subj xsi:type="NPPhraseSpec">
						<head cat="NOUN" base="patient" />
						<spec xsi:type="WordElement" cat="DETERMINER" base="the" />
					</subj>
					<vp xsi:type="VPPhraseSpec" TENSE="PAST">
						<head cat="VERB" base="lie" />
						<postMod xsi:type="VPPhraseSpec" TENSE="PAST">
							<head cat="VERB" base="etherise" />
							<postMod xsi:type="PPPhraseSpec">
								<head cat="PREPOSITION" base="upon" />
								<compl xsi:type="NPPhraseSpec">
									<head cat="NOUN" base="table" />
									<spec xsi:type="WordElement" cat="DETERMINER" base="a" />
								</compl>
							</postMod>
						</postMod>
					</vp>
				</child>
				<child xsi:type="SPhraseSpec">
					<subj xsi:type="NPPhraseSpec">
						<head cat="NOUN" base="patient" />
						<spec xsi:type="WordElement" cat="DETERMINER" base="the" />
					</subj>
					<vp xsi:type="VPPhraseSpec" TENSE="PAST">
						<head cat="VERB" var="IRREGULAR" base="lie" />
						<postMod xsi:type="VPPhraseSpec" TENSE="PAST">
							<head cat="VERB" base="etherise" />
							<postMod xsi:type="PPPhraseSpec">
