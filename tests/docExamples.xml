<nlg:NLGSpec xmlns="http://code.google.com/p/simplenlg/schemas/version1"
	xmlns:nlg="http://code.google.com/p/simplenlg/schemas/version1"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://code.google.com/p/simplenlg/schemas/version1 RealizerSchema.xsd ">

	<Recording name="cch examples">

		<Record name="doc example 1">
			<Document>
				<child xsi:type="SPhraseSpec">
					<subj xsi:type="NPPhraseSpec">
						<head cat="NOUN" base="transfusion of whole blood"/>
					</subj>
					<vp xsi:type="VPPhraseSpec" PASSIVE="true" TENSE="PRESENT">
						<head cat="VERB" base="indicate"/>
					</vp>
				</child>
			</Document>
		</Record>

		<Record name="doc example 2">
			<Document>
				<child xsi:type="SPhraseSpec">
					<preMod xsi:type="PPPhraseSpec">
						<head cat="PREPOSITION" base="as a result of"/>
						<compl xsi:type="NPPhraseSpec">
							<head cat="NOUN" base="procedure"/>
							<spec xsi:type="WordElement" cat="DETERMINER" base="the"/>
						</compl>
					</preMod>
					<subj xsi:type="NPPhraseSpec">
						<head cat="NOUN" base="patient"/>
						<spec xsi:type="WordElement" cat="DETERMINER" base="the"/>
					</subj>
					<vp xsi:type="CoordinatedPhraseElement" conj="and">
						<coord xsi:type="VPPhraseSpec" TENSE="PAST">
							<head cat="VERB" base="have"/>
							<compl xsi:type="NPPhraseSpec">
								<head cat="NOUN" base="adverse contrast media reaction"/>
								<spec xsi:type="WordElement" cat="DETERMINER" base="a"/>
							</compl>
						</coord>
						<coord xsi:type="VPPhraseSpec" TENSE="PAST">
							<head cat="VERB" base="have"/>
							<compl xsi:type="NPPhraseSpec">
								<head cat="NOUN" base="decreased platelet count"/>
								<spec xsi:type="WordElement" cat="DETERMINER" base="a"/>
							</compl>
						</coord>
						<coord xsi:type="VPPhraseSpec" TENSE="PAST">
							<head cat="VERB" base="go"/>
							<postMod xsi:type="PPPhraseSpec">
								<head cat="PREPOSITION" base="into"/>
								<compl xsi:type="NPPhraseSpec">
									<head cat="NOUN" base="cardiogenic shock"/>
								</compl>
							</postMod>
						</coord>
					</vp>
				</child>
			</Document>
		</Record>

		<Record name="doc example 3">
			<Document>
				<child xsi:type="DocumentElement" cat="LIST_ITEM"
					xsi:schemaLocation="http://code.google.com/p/simplenlg/schemas/version1 ">
					<child xsi:type="DocumentElement" cat="SENTENCE"
						xsi:schemaLocation="http://code.google.com/p/simplenlg/schemas/version1">
						<child xsi:type="NPPhraseSpec">
							<head cat="NOUN" base="normal coronary arteries"/>
						</child>
					</child>
				</child>
				<child xsi:type="DocumentElement" cat="LIST_ITEM"
					xsi:schemaLocation="http://code.google.com/p/simplenlg/schemas/version1">
					<child xsi:type="DocumentElement" cat="SENTENCE"
						xsi:schemaLocation="http://code.google.com/p/simplenlg/schemas/version1">
						<child xsi:type="NPPhraseSpec">
							<head cat="NOUN" base="normal left heart hemodynamics"/>
						</child>
					</child>
				</child>
				<child xsi:type="DocumentElement" cat="LIST_ITEM"
					xsi:schemaLocation="http://code.google.com/p/simplenlg/schemas/version1">
					<child xsi:type="DocumentElement" cat="SENTENCE"
						xsi:schemaLocation="http://code.google.com/p/simplenlg/schemas/version1">
						<child xsi:type="NPPhraseSpec">
							<head cat="NOUN" base="normal right heart hemodynamics"/>
						</child>
					</child>
				</child>
			</Document>
		</Record>

		<Record name="doc example 4">
			<Document>
				<child xsi:type="SPhraseSpec">
					<subj xsi:type="NPPhraseSpec">
						<head cat="ADVERB" base="there"/>
					</subj>
					<vp xsi:type="VPPhraseSpec">
						<head cat="VERB" base="be"/>
						<compl xsi:type="NPPhraseSpec">
							<preMod xsi:type="CoordinatedPhraseElement" conj=",">
								<coord xsi:type="AdjPhraseSpec">
									<head cat="ADJECTIVE" base="eccentric"/>
								</coord>
								<coord xsi:type="AdjPhraseSpec">
									<head cat="ADJECTIVE" base="tubular"/>
								</coord>
							</preMod>
							<head cat="NOUN" base="restenosis"/>
							<postMod xsi:type="StringElement">
								<val>(18 mm x 1 mm)</val>
							</postMod>
							<spec xsi:type="WordElement" cat="DETERMINER" base="a"/>
						</compl>
						<postMod xsi:type="PPPhraseSpec">
							<preMod xsi:type="VPPhraseSpec" FORM="GERUND">
								<head cat="VERB" base="extend"/>
							</preMod>
							<head cat="PREPOSITION" base="from"/>
							<compl xsi:type="NPPhraseSpec">
								<preMod xsi:type="AdjPhraseSpec">
									<head cat="ADJECTIVE" base="proximal"/>
								</preMod>
								<spec xsi:type="WordElement" cat="DETERMINER" base="the"/>
							</compl>
							<postMod xsi:type="PPPhraseSpec">
								<head cat="PREPOSITION" base="to"/>
								<compl xsi:type="NPPhraseSpec">
									<preMod xsi:type="AdjPhraseSpec">
										<head cat="ADJECTIVE" base="mid"/>
									</preMod>
									<head cat="NOUN" base="right coronary artery"/>
									<spec xsi:type="WordElement" cat="DETERMINER" base="the"/>
								</compl>
							</postMod>
						</postMod>
						<postMod xsi:type="PPPhraseSpec">
							<head cat="PREPOSITION" base="with"/>
							<compl xsi:type="NPPhraseSpec">
								<head cat="NOUN" base="TIMI 1 flow"/>
							</compl>
						</postMod>
					</vp>
				</child>
			</Document>
		</Record>

	</Recording>
</nlg:NLGSpec>
